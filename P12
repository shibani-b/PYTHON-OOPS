from abc import ABC, abstractmethod

class vehical(ABC):

    @abstractmethod
    def start_engine(self):
        pass

    @abstractmethod
    def stop_engine(self):
        pass

class car(vehical):

    def start_engine(self):
        print("starting car")

    def stop_engine(self):
        print("stopping car")    

class bike(vehical):

    def start_engine(self):
        print("starting bike")

    def stop_engine(self):
        print("stopping bike") 


c1=car()
c1.start_engine()
c1.stop_engine()
b1=bike()
b1.start_engine()
b1.stop_engine()  

      

    
    



