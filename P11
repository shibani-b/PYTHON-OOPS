from abc import ABC,abstractmethod

class shape(ABC):
    @abstractmethod
    def area(self):
        pass

class circle(shape):
    def area(self,a):
        print(3.14 * a * a)

class rectangle(shape):
    def area(self,a,b):
        print(a*b)      
    

c1=circle()
r1=rectangle()
c1.area(5)
r1.area(5,4)
