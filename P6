class Car:

    def __init__(self,brand,speed):
        self.brand=brand
        self.__speed=speed

    def getspeed(self):
        return self.__speed    
        
    def setspeed(self,newspeed):
        self.__speed = newspeed  

    def __str__(self):
        return f"{self.brand}\n{self.getspeed()}"
    

c1= Car("swift",100)        
print(c1)
c1.setspeed(200)
print(c1)
