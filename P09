class Laptop:

    def __init__(self, price, brand,model):
        self.__price=price
        self.brand=brand
        self.model=model

    def __del__(self):
        print("This is a destructor")    

    def getprice(self):
        return self.__price

    def setprice(self,newprice):
        if newprice<0:
            print("Invalid price")

        else:    
            self.__price=newprice

    def __str__(self):
        return f"price:{self.__price}\nbrand:{self.brand}\nmodel:{self.model}"  

l1=Laptop(30000,"lenovo",23)             
print(l1)

             
