class Student:

    def __init__(self,name,rollno):
        self.name = name
        self.rollno = rollno
        

    class Marks:
    
        def __init__(self,sub,score):
            self.sub = sub
            self.score = score
                


s1 = Student("Ram",12)
o1= Student.Marks("Eng",90)
print(s1.name,s1.rollno,o1.sub,o1.score)

